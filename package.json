{
    "name": "ctree",
    "displayName": "ctree",
    "description": "Build calling tree based on cscope",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
                "command": "ctree.build",
                "title": "ctree build"
            },
            {
                "command": "ctree.findcaller",
                "title": "ctree show"
            },
            {
                "command": "ctree.gotodef",
                "title": "goto def"
            },
            {
                "command": "ctree.gotoline",
                "title": "goto line"
            }
        ],
        "viewsContainers": {
            "activitybar": [{
                "id": "ctree",
                "title": "ctree",
                "icon": "./media/tree.svg"
            }]
        },
        "views": {
            "ctree": [{
                "id": "ctreeview",
                "name": "hierarchy",
                "contextualTitle": "tree Explorer"
            }]
        },
        "menus": {
            "view/item/context": [{
                    "command": "ctree.gotodef",
                    "when": "view == ctreeview",
                    "group": "inline"
                },
                {
                    "command": "ctree.gotoline",
                    "when": "view == ctreeview",
                    "group": "inline"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }
}